                closeModal();
                alert("Please Select Transporter...!");
                $(".TRNSPRTNAME").focus();
                return false;
            }
            if (temp3 == "") {
                closeModal();
                alert("Please Select Transporter...!");
                $(".TRNSPRTNAME").focus();
                return false;
            }
            vtemp3 = $("#VHLNO").val();
            if (vtemp3 == "") {
                closeModal();
                alert("Please Enter Vehicle No....!");
                $(".VHLNO").focus();
                return false;
            }
            temp4 = $("#VSLID").val();
            if (temp4 == "0") {
                closeModal();
                alert("Please Select Vessel");
                $(".VSLNAME").focus();
                return false;
            }
            temp1 = $("#IMPRTID").val();
            if (temp1 == "0") {
                closeModal();
                alert("Please Select Importer");
                $(".IMPRTNAME").focus();
                return false;
            }
            temp2 = $("#STMRID").val();
            if (temp2 == "0") {
                closeModal();
                alert("Please Select Streamer");
                $(".STMRNAME").focus();
                return false;
            }
            var vno = $("#GPETYPE").val();
            var amt = $("#GPEAMT").val();
            if (vno == 1 && amt == 0) {
                closeModal();
                alert("Please Enter Escord Amount...!");
                $(".GPEAMT").focus();
                return false;
            }
            else {
                return true;
            }
            var wgt = $("#GPWTYPE").val();
