                                        </div>
                                        <div class="col-md-1">
                                            <div class="form-group">
                                                <label>No  </label>
                                                @Html.TextBoxFor(model => model.masterdata[0].OSMNO, new { @class = "form-control  OSMNO", @id = "OSMNO", @readonly = true })
