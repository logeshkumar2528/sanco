                                        <input type="button" name="save2" value="Submit" id="btnsave" class="btn btn-success btn-xs" tabindex="15" />
                                        <input type="submit" name="submit" value="Submit" class="btn btn-success btn-xs hide" id="save" tabindex="15" />
